<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.forbes</groupId>
      <artifactId>forbes-order</artifactId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  <groupId>org.forbes</groupId>
  <artifactId>order-client</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>order-client</name>
  <url>http://maven.apache.org</url>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencies>
    <dependency>
      <groupId>org.forbes</groupId>
      <artifactId>order-comm</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </dependency>
	<dependency>
      <groupId>org.forbes</groupId>
      <artifactId>order-dal</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.forbes</groupId>
      <artifactId>order-biz</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.forbes</groupId>
      <artifactId>order-config</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </dependency>
      <dependency>
          <groupId>org.springframework.cloud</groupId>
          <artifactId>spring-cloud-openfeign-core</artifactId>
          <version>2.0.0.RELEASE</version>
      </dependency>
  </dependencies>
  <build>
  	<plugins>
		<!-- 打包跳过测试 -->
        <plugin>
        	<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-maven-plugin</artifactId>
	        <configuration>
						<mainClass>org.forbes.ForbesApplication</mainClass>
			</configuration>
		<dependencies>
			<dependency>
	         	<groupId>org.forbes</groupId>
	         	<artifactId>order-dal</artifactId>
	         	<version>0.0.1-SNAPSHOT</version>
	         	<scope>system</scope>
	         	<systemPath>${parentPath}order-dal/target/order-dal-0.0.1-SNAPSHOT.jar</systemPath>
	         </dependency>
	         <dependency>
	         	<groupId>org.forbes</groupId>
	         	<artifactId>order-comm</artifactId>
	         	<version>0.0.1-SNAPSHOT</version>
	         	<scope>system</scope>
	         	<systemPath>${parentPath}order-comm/target/order-comm-0.0.1-SNAPSHOT.jar</systemPath>
	         </dependency>
	         <dependency>
	         	<groupId>org.forbes</groupId>
	         	<artifactId>order-client</artifactId>
	         	<version>0.0.1-SNAPSHOT</version>
	         	<scope>system</scope>
	         	<systemPath>${parentPath}order-client/target/order-client-0.0.1-SNAPSHOT.jar</systemPath>
	         </dependency>
	         <dependency>
	         	<groupId>org.forbes</groupId>
	         	<artifactId>order-biz</artifactId>
	         	<version>0.0.1-SNAPSHOT</version>
	         	<scope>system</scope>
	         	<systemPath>${parentPath}order-biz/target/order-biz-0.0.1-SNAPSHOT.jar</systemPath>
	         </dependency>
		</dependencies>
	</plugin>
  	</plugins>
  </build>
</project>
